{
  "name": "CueMe",
  "version": "1.0.112",
  "main": "./dist-electron/main.js",
  "scripts": {
    "clean": "rimraf dist dist-electron",
    "dev": "vite",
    "build:win": "npm run clean && tsc -p electron/tsconfig.json && vite build",
    "build:linux": "npm run clean && tsc -p electron/tsconfig.json && vite build",
    "build:native": "bash scripts/build-native-binary.sh",
    "build": "npm run clean && npm run build:native && tsc -p electron/tsconfig.json && vite build",
    "preview": "vite preview",
    "electron:dev": "tsc -p electron/tsconfig.json && electron .",
    "start": "concurrently \"npm run dev\" \"wait-on http://localhost:5181 && npm run electron:dev\"",
    "app:dev": "concurrently \"vite\" \"wait-on http://localhost:5173 && cross-env electron .\"",
    "app:build": "npm run build && electron-builder",
    "app:build:all": "npm run build && electron-builder --mac --win --linux",
    "app:build:mac": "npm run build && electron-builder --mac",
    "app:build:win": "npm run build:win && electron-builder --win",
    "app:build:linux": "npm run build:linux && electron-builder --linux",
    "release": "npm run build && electron-builder --publish=always",
    "release:draft": "npm run build && electron-builder --publish=onTagOrDraft",
    "watch": "tsc -p electron/tsconfig.json --watch",
    "test-model": "node test-model.js"
  },
  "build": {
    "appId": "com.cueme.interview-assistant",
    "productName": "CueMe",
    "copyright": "Copyright Â© 2024 CueMe",
    "protocols": [
      {
        "name": "cueme-protocol",
        "schemes": [
          "cueme"
        ]
      }
    ],
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "dist-native/**/*",
      "custom-binaries/**/*",
      "package.json"
    ],
    "directories": {
      "output": "release"
    },
    "compression": "maximum",
    "afterPack": "./scripts/afterPack.js",
    "afterSign": "./scripts/afterSign.js",
    "mac": {
      "category": "public.app-category.productivity",
      "icon": "assets/logo.icns",
      "minimumSystemVersion": "11.0",
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "assets/entitlements.mac.plist",
      "notarize": {
        "teamId": "${APPLE_TEAM_ID}"
      },
      "extendInfo": {
        "NSMicrophoneUsageDescription": "CueMe needs microphone access to listen for interview questions and provide real-time coding assistance.",
        "NSAudioCaptureUsageDescription": "CueMe needs system audio access to capture audio from video calls (Zoom, Teams, etc.) for better interview question detection.",
        "NSScreenCaptureDescription": "CueMe uses screen recording to capture system audio from video calls (Zoom, Teams, etc.) for better interview question detection. No visual recording is performed - only audio is captured."
      }
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "publisherName": "CueMe",
      "verifyUpdateCodeSignature": false
    },
    "linux": {
      "target": [
        {
          "target": "AppImage",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "deb",
          "arch": [
            "x64"
          ]
        }
      ],
      "category": "Development"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "CueMe"
    },
    "extraResources": [
      ".env",
      {
        "from": "custom-binaries/audiotee",
        "to": "audiotee"
      }
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "itsukison",
        "repo": "CueMeFinal",
        "releaseType": "release"
      }
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "AI-powered interview assistant for coding interviews",
  "devDependencies": {
    "@electron-forge/publisher-github": "^7.9.0",
    "@types/color": "^4.2.0",
    "@types/diff": "^6.0.0",
    "@types/electron": "^1.4.38",
    "@types/node": "^22.9.0",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/screenshot-desktop": "^1.12.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^8.14.0",
    "@typescript-eslint/parser": "^8.14.0",
    "@vitejs/plugin-react": "^4.3.3",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "cross-env": "^7.0.3",
    "electron": "^33.2.0",
    "electron-builder": "^25.1.8",
    "electron-is-dev": "^3.0.1",
    "postcss": "^8.4.49",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.6.3",
    "vite": "^5.4.11",
    "vite-plugin-electron": "^0.28.8",
    "vite-plugin-electron-renderer": "^0.14.6",
    "wait-on": "^8.0.1"
  },
  "dependencies": {
    "@google/genai": "^1.25.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-toast": "^1.2.2",
    "@specy/liquid-glass-react": "^1.0.2",
    "@supabase/supabase-js": "^2.56.0",
    "audiotee": "^0.0.6",
    "axios": "^1.7.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "diff": "^7.0.0",
    "dotenv": "^16.4.5",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.6.2",
    "form-data": "^4.0.1",
    "lucide-react": "^0.460.0",
    "openai": "^5.15.0",
    "react": "^18.3.1",
    "react-code-blocks": "^0.1.6",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-query": "^3.39.3",
    "react-syntax-highlighter": "^15.6.1",
    "screenshot-desktop": "^1.15.0",
    "sharp": "^0.33.5",
    "tailwind-merge": "^2.5.4",
    "tesseract.js": "^5.0.5",
    "uuid": "^11.0.3"
  }
}
